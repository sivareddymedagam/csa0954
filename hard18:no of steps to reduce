import java.util.Scanner;
class return_num
{
	public static void main(String[] args)
	{
		Scanner input=new Scanner(System.in);
		System.out.println("enter the number");

		int n=input.nextInt();
		int sum=0;
		while(n>0)
		{
			if(n%2==0)
			{
				n=n/2;
			}
			else
			{
				n=n-1;
			}
		sum=sum+1;
		}
		System.out.println(sum);
	}
}
