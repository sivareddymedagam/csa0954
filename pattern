class pattern_numk
{
	public static void main(String[] args)
	{
		System.out.println("the the number of rows:");
		int n=Integer.parseInt(System.console().readLine());
		int k=1;
		 for(int i=1;i<=n;i++)
		{
			 for(int j=1;j<=i;j++)
			{
				 System.out.print(k*k+ " ");
				 k++;
			}
			System.out.println();
		}
	}
}
